{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aphrodite": "^1.2.5",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "enzyme-to-json": "^3.3.1",
    "history": "^4.7.2",
    "linearicons": "^1.0.1",
    "lodash": "^4.17.5",
    "polished": "^1.9.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-onclickoutside": "^6.7.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.2",
    "redux-devtools": "^3.4.1",
    "redux-form": "^7.2.3",
    "redux-observable": "^0.18.0",
    "redux-responsive": "^4.3.5",
    "rxjs": "^5.5.6",
    "typeface-rubik": "^0.0.54"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "flow-bin": "^0.68.0",
    "flow-typed": "^2.4.0",
    "husky": "^0.15.0-rc.8",
    "lint-staged": "^7.0.0",
    "nock": "^9.2.3",
    "react-test-renderer": "^16.2.0",
    "standard": "^11.0.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "globals": [
      "it",
      "describe",
      "expect",
      "jest",
      "beforeEach",
      "afterEach"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "standard --fix",
      "git add"
    ],
    "src/**/*.test.js": [
      "standard --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "make lint && make typecheck && make test-ci"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
